    <div class="modal" id="imageCropPopup" [style.display]="showModalCrop ? 'block' : 'none'">
        <div class="modal-dialog">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">

                    <button type="button" class="close" data-dismiss="modal" (click)="hideModalCrop()">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">

                    <div class="col-md-12 col-sm-12 col-xs-12 pb-2" style="margin-top: 0">
                        <div class="col-md-6 col-sm-6 col-xs-12 p-2 mt-2 leftCon">
                            <span class="instruct">* File format required JPEG, size should not be more than 3MB and resolution must be equal to or greater than 300dpi.  </span>
                            <div class="imgCon">

                                <!-- <image-cropper
                                  [style.display]="showCropper ? null : 'none'"
                                  [imageChangedEvent]="imageurl"
                                    [maintainAspectRatio]="false"
                                    [cropper]="cropper"

                                   (imageLoaded)="imageLoaded()"
                                ></image-cropper> -->
                                <img [src]="imageurl" />
                            </div>
                        </div>

                        <div class="col-md-6 col-sm-6 col-xs-12 rightCon">
                            <div class="mt-3">

                                <input type="file" (change)="Filechangeevent($event,1)" />


                            </div>
                            <div class="imageContainer">
                                <img [src]="Cropimageurl" />
                            </div>
                            <input type="button" class="btn btn-success pull-right mr-2" (click)="CropMandate()" value="Crop" *ngIf="btnCrop" [attr.disabled]="btncropImagedisabled ? '' : null" />
                            <input type="button" class="btn btn-success pull-right mr-2" (click)="UploadMandate()" value="Upload" *ngIf="btnupload" />

                            <label id="lblName">{{lblmsg}}</label>
                            <input type="button" class="btn btn-success pull-right mr-2" data-dismiss="modal" (click)="hideModalCrop()" value="Ok" *ngIf="btnok" />
                            <div class="details">
                                <h5>Mandate Details</h5>
                                <p><span class="heading">Reference 1:</span><span>{{Referenceno}} </span></p>
                                <p><span class="heading">Account No.:</span><span>{{AccountNumber}}</span></p>
                                <p><span class="heading">Customer Name:</span><span>{{CustomerName}}</span></p>
                            </div>
                        </div>


                    </div>
                </div>


            </div>
        </div>
    </div>

